set architecture "LIFCL"
set device "LIFCL-33U"
set package "FCCSP104"
set speed "7_High-Performance_1.0V"
set WRAPPER_INST "lscc_pixel2byte_inst"
set FAMILY "LIFCL"
set DSI_FORMAT 0
set DSI_MODE "NONBURST_PULSES"
set DATA_TYPE "YUV422_8"
set DT "6'h1E"
set PIX_WIDTH 8
set DATA_WIDTH 16
set NUM_TX_LANE 2
set NUM_PIX_LANE 2
set TX_GEAR 8
set MISCON 1
set TXFR_SIG 1


create_clock -name {pix_clk_i} -period 5 [get_ports pix_clk_i]
create_clock -name {byte_clk_i} -period 5 [get_ports byte_clk_i]